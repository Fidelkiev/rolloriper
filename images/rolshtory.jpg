<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="fabric" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f39c12;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e67e22;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Window -->
  <rect x="50" y="30" width="300" height="240" fill="#e8f4f8" stroke="#8b7355" stroke-width="6"/>
  <line x1="200" y1="30" x2="200" y2="270" stroke="#8b7355" stroke-width="3"/>
  
  <!-- Roller blind -->
  <rect x="60" y="40" width="130" height="220" fill="url(#fabric)" opacity="0.9"/>
  <rect x="210" y="40" width="130" height="220" fill="url(#fabric)" opacity="0.9"/>
  
  <!-- Roller tubes -->
  <rect x="55" y="35" width="140" height="8" fill="#34495e" rx="4"/>
  <rect x="205" y="35" width="140" height="8" fill="#34495e" rx="4"/>
  
  <!-- Bottom rails -->
  <rect x="60" y="255" width="130" height="5" fill="#2c3e50"/>
  <rect x="210" y="255" width="130" height="5" fill="#2c3e50"/>
  
  <!-- Title -->
  <text x="200" y="295" text-anchor="middle" font-family="Arial" font-size="16" fill="#2c3e50">Рольшторы</text>
</svg>
