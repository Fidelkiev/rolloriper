<svg width="300" height="250" viewBox="0 0 300 250" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="ecoFabric" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#27ae60;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2ecc71;stop-opacity:1" />
    </linearGradient>
    <pattern id="ecoPleats" x="0" y="0" width="15" height="25" patternUnits="userSpaceOnUse">
      <path d="M0,0 Q7.5,12.5 0,25" stroke="#27ae60" stroke-width="1.5" fill="none" opacity="0.7"/>
      <path d="M15,0 Q7.5,12.5 15,25" stroke="#27ae60" stroke-width="1.5" fill="none" opacity="0.7"/>
    </pattern>
  </defs>
  
  <!-- Window frame -->
  <rect x="30" y="20" width="240" height="180" fill="#e8f4f8" stroke="#8b7355" stroke-width="6"/>
  <line x1="150" y1="20" x2="150" y2="200" stroke="#8b7355" stroke-width="3"/>
  <line x1="30" y1="110" x2="270" y2="110" stroke="#8b7355" stroke-width="3"/>
  
  <!-- Eco pleated blinds -->
  <rect x="40" y="30" width="220" height="160" fill="url(#ecoPleats)" opacity="0.8"/>
  <rect x="40" y="30" width="220" height="160" fill="url(#ecoFabric)" opacity="0.3"/>
  
  <!-- Control mechanism -->
  <rect x="145" y="210" width="10" height="20" fill="#34495e" rx="2"/>
  <circle cx="150" cy="235" r="8" fill="#e74c3c"/>
  
  <!-- Title -->
  <text x="150" y="245" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">Плиссе Эко</text>
</svg>
