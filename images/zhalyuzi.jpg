<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="blind" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8e44ad;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9b59b6;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Window frame -->
  <rect x="50" y="30" width="300" height="240" fill="#e8f4f8" stroke="#8b7355" stroke-width="6"/>
  
  <!-- Horizontal blinds -->
  <g id="slats">
    <rect x="60" y="50" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="70" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="90" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="110" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="130" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="150" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="170" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="190" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="210" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="230" width="280" height="8" fill="url(#blind)" rx="2"/>
    <rect x="60" y="250" width="280" height="8" fill="url(#blind)" rx="2"/>
  </g>
  
  <!-- Cords -->
  <line x1="150" y1="40" x2="150" y2="270" stroke="#34495e" stroke-width="2"/>
  <line x1="250" y1="40" x2="250" y2="270" stroke="#34495e" stroke-width="2"/>
  
  <!-- Title -->
  <text x="200" y="295" text-anchor="middle" font-family="Arial" font-size="16" fill="#2c3e50">Жалюзи</text>
</svg>
