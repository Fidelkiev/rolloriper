<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="awning" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#e74c3c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#c0392b;stop-opacity:1" />
    </linearGradient>
    <pattern id="stripes" x="0" y="0" width="40" height="20" patternUnits="userSpaceOnUse">
      <rect x="0" y="0" width="20" height="20" fill="url(#awning)"/>
      <rect x="20" y="0" width="20" height="20" fill="#ffffff" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Building wall -->
  <rect x="0" y="100" width="400" height="200" fill="#bdc3c7"/>
  
  <!-- Awning frame -->
  <path d="M50,100 Q200,50 350,100" stroke="#34495e" stroke-width="4" fill="none"/>
  
  <!-- Awning fabric -->
  <path d="M50,100 Q200,80 350,100 L350,140 Q200,120 50,140 Z" fill="url(#stripes)"/>
  
  <!-- Support arms -->
  <line x1="80" y1="100" x2="80" y2="180" stroke="#34495e" stroke-width="6"/>
  <line x1="320" y1="100" x2="320" y2="180" stroke="#34495e" stroke-width="6"/>
  
  <!-- Valance -->
  <rect x="50" y="140" width="300" height="20" fill="url(#stripes)"/>
  
  <!-- Table and chairs (cafe setting) -->
  <rect x="150" y="200" width="100" height="60" fill="#8b6f47" rx="5"/>
  <circle cx="130" cy="220" r="15" fill="#2c3e50"/>
  <circle cx="270" cy="220" r="15" fill="#2c3e50"/>
  
  <!-- Title -->
  <text x="200" y="295" text-anchor="middle" font-family="Arial" font-size="16" fill="#2c3e50">Маркизы</text>
</svg>
