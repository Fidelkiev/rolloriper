<svg width="300" height="250" viewBox="0 0 300 250" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="duoFabric1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3498db;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#2980b9;stop-opacity:0.8" />
    </linearGradient>
    <linearGradient id="duoFabric2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ecf0f1;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#bdc3c7;stop-opacity:0.8" />
    </linearGradient>
    <pattern id="duoPleats" x="0" y="0" width="20" height="30" patternUnits="userSpaceOnUse">
      <path d="M0,0 Q10,15 0,30" stroke="#3498db" stroke-width="2" fill="none" opacity="0.6"/>
      <path d="M20,0 Q10,15 20,30" stroke="#3498db" stroke-width="2" fill="none" opacity="0.6"/>
    </pattern>
  </defs>
  
  <!-- Modern window frame -->
  <rect x="30" y="20" width="240" height="180" fill="#e8f4f8" stroke="#34495e" stroke-width="6"/>
  <line x1="150" y1="20" x2="150" y2="200" stroke="#34495e" stroke-width="3"/>
  <line x1="30" y1="110" x2="270" y2="110" stroke="#34495e" stroke-width="3"/>
  
  <!-- Double layer pleated blinds -->
  <!-- Back layer (light) -->
  <rect x="40" y="30" width="220" height="160" fill="url(#duoFabric2)" opacity="0.7"/>
  
  <!-- Front layer (blue) -->
  <rect x="40" y="30" width="220" height="160" fill="url(#duoPleats)" opacity="0.8"/>
  <rect x="40" y="30" width="220" height="160" fill="url(#duoFabric1)" opacity="0.5"/>
  
  <!-- Dual control system -->
  <rect x="135" y="210" width="8" height="20" fill="#34495e" rx="2"/>
  <rect x="157" y="210" width="8" height="20" fill="#34495e" rx="2"/>
  <circle cx="139" cy="235" r="5" fill="#3498db"/>
  <circle cx="161" cy="235" r="5" fill="#95a5a6"/>
  
  <!-- Duo indicator -->
  <circle cx="250" cy="30" r="8" fill="#3498db"/>
  <text x="250" y="35" text-anchor="middle" font-family="Arial" font-size="10" font-weight="bold" fill="#fff">D</text>
  
  <!-- Title -->
  <text x="150" y="245" text-anchor="middle" font-family="Arial" font-size="14" font-weight="bold" fill="#2c3e50">Плиссе Дуо</text>
</svg>
